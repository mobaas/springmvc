<hibernate-mapping>

<class name="com.mobaas.ssfx.domain.Info" table="info" dynamic-update="true">
	<id name="id" type="java.lang.Integer">
		<column name="id" />
		<generator class="native" />
	</id>
	<property name="title" type="java.lang.String">
		<column name="title" />
	</property>
	<property name="content" type="java.lang.String">
		<column name="content" />
	</property>
	<property name="category" type="java.lang.Integer">
		<column name="category" />
	</property>
	<property name="pubTime" type="java.util.Date">
		<column name="pub_time" />
	</property>
	<property name="topNo" type="java.lang.Integer">
		<column name="top_no" />
	</property>
</class>

</hibernate-mapping>