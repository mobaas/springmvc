<?xml version="1.0" encoding="utf-8" ?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd" updateCheck="false">

	<cache name="infoCache" eternal="false" maxElementsInMemory="100" overflowToDisk="false" 
		diskPersistent="false" timeToIdleSeconds="0" timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU" />
		
	<cache name="adminlogCache" eternal="false" maxElementsInMemory="100" overflowToDisk="false" 
		diskPersistent="false" timeToIdleSeconds="0" timeToLiveSeconds="300" memoryStoreEvictionPolicy="LRU" />
				
</ehcache>